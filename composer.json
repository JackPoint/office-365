  "require": {
    "php": "^7.4",
    "digitickets/lalit": "^3.1",
    "guzzlehttp/guzzle": "^7.4",
    "jms/serializer": "^3.17",
    "symfony/yaml": "^6.0"
  },
  "require-dev": {
